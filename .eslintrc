{
    "plugins": [
        "prettier"
    ],
    "extends": [
        "plugin:prettier/recommended"
    ],
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {},
        "rules": {
            "strict": 0,
            "prettier/prettier": [
                "error",
                {
                    "singleQuote": true,
                    "trailingComma": "all",
                    "printWidth": 120,
                    "tabWidth": 4,
                    "endOfLine": "auto"
                }
            ]
        }
    }
}